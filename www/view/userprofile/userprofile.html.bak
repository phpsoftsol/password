<div class="container">
    <div class="row">
        <form role="form" class="form-signin" ng-app="providerApp" ng-controller="profileController" ng-init="init()">
            <div class="col-lg-6">
               
               <h1 class="form-signin-heading" align="center">userprofile</h1>
                <div class="form-group">
                    <label for="InputEmail"> Email  </label>
                    <div class="input-group">
                        <input type="email" class="form-control" id="InputEmailFirst" name="InputEmail" ng-model="userdetail.email" required>
                        <span class="input-group-addon"><span class="glyphicon glyphicon-asterisk"></span></span>
                    </div>
                </div>
                <div class="form-group">
                    <label for="InputName">firstname </label>
                    <div class="input-group">
                        <input type="text" class="form-control" name="InputName" id="InputName" ng-model="userdetail.firstname"    >
                        <span class="input-group-addon"><span class="glyphicon glyphicon-asterisk"></span></span>
                    </div>
              <div class="form-group">
                    <label for="InputName">lastname </label>
                    <div class="input-group">
                        <input type="text" class="form-control" name="InputPassword" id="InputPassword" ng-model="userdetail.lastname"   required>
                        <span class="input-group-addon"><span class="glyphicon glyphicon-asterisk"></span></span>
                    </div>
				<div class="form-group">
                    <label for="InputName">Phoneno</label>
                    <div class="input-group">
                        <input type="text" class="form-control" name="InputPhoneno" id="InputPhoneno" ng-model="userdetail.phoneno">
                        <span class="input-group-addon"><span class="glyphicon glyphicon-asterisk"></span></span>
                    </div>
					</div>
					
                                  </div>
								  </form>
   <button class="btn btn-lg btn-primary btn-block" type="button" ng-click="update()" >update</button>
        </form>
    </div>
</div>

